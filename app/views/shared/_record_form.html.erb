      <% @recorder.options.each do |option| %>
<%= form_for(@record, url: option_records_path(option)) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
    <div class="form">
        <%= f.submit option.name, class: "form-control btn btn-primary" %>
    </div>
<% end %>
      <% end %>
